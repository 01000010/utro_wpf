<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:utro_wpf">
    <!-- Colors -->
    <Color x:Key="Main1">#B5D5CA</Color>
    <Color x:Key="Main2">#E0A9AF</Color>
    <Color x:Key="Extra1">#D1EEFC</Color>
    <Color x:Key="Extra2">#FFFCD6</Color>
    <Color x:Key="Text1">#FFFFFF</Color>
    <Color x:Key="Text2">#000000</Color>
    <!-- Brushes -->
    <SolidColorBrush x:Key="Main1Brush" Color="{StaticResource Main1}"/>
    <SolidColorBrush x:Key="Main2Brush" Color="{StaticResource Main2}"/>
    <SolidColorBrush x:Key="Extra1Brush" Color="{StaticResource Extra1}"/>
    <SolidColorBrush x:Key="Extra2Brush" Color="{StaticResource Extra2}"/>
    <SolidColorBrush x:Key="Text1Brush" Color="{StaticResource Text1}"/>
    <SolidColorBrush x:Key="Text2Brush" Color="{StaticResource Text2}"/>
</ResourceDictionary>